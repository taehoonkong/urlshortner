<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8">
    <title></title>
    <link rel="stylesheet" href="/static/index.css">
  </head>
  <body>
    <h1>Hello Express</h1>
    <form action="/" method="post">
      <input type="url" name="longUrl">
      <input type="submit" value="전송">
    </form>
    <table>
      <thead>
        <tr>
          <td>Long URL</td>
          <td>Short URL</td>
        </tr>
      </thead>
      <tbody>
        <% data.forEach(item => { %>
          <tr>
            <td><%= item.longUrl %></td>
            <td><a href="/<%= item.id %>" target="_blank"><%= item.id %></a></td>
          </tr>
        <% }) %>
      </tbody>
    </table>
  </body>
</html>
